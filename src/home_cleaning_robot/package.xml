<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>home_cleaning_robot</name>
  <version>0.0.1</version>
  <description>Home Cleaner Bot Robot URDF Package</description>
  <maintainer email="mehmetsefatoksoy@istun.edu.tr">Mehmet Sefa Toksoy</maintainer>
  <maintainer email="elifceydacivelek@istun.edu.tr">Elif Ceyda Civelek</maintainer>
  <license>Apache-2.0</license>

  <depend>rclpy</depend>
  <depend>sensor_msgs</depend>
  <depend>robot_state_publisher</depend>
  <depend>joint_state_publisher_gui</depend>
  
  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <exec_depend>launch</exec_depend>
